/* pages/loading/loading.wxss */
.loading-container{

  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  position: absolute;
  white-space:pre-wrap;
}

.enter-container{
  width: 100%;
}
.enter-container image{
  width: 358rpx;
  height: auto;
  text-align: center;
}

.enter-container button{
  margin-top: 60rpx;
}

button{
  width: 300rpx;
  text-align: left;
  height: 80rpx;
  padding: 0px;
  line-height: 25rpx;
  border-radius:8rpx;
}